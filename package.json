{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve": "http-server dist --silent -p 8080 --push-state",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "concurrently --success first --kill-others \"yarn serve\" \"cypress run\"",
    "test:e2e:w": "concurrently --success first --kill-others \"yarn serve\" \"cypress open\"",
    "test:unit": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} mocha -r ts-node/register src/**/*.spec.ts",
    "test:unit:w": "yarn run test:unit --watch",
    "test:prod": "cross-env CYPRESS_baseUrl=https://tygr.info cypress run"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@redux-saga/testing-utils": "^1.0.4",
    "@types/chai": "^4.1.0",
    "@types/gsap": "^1.20.2",
    "@types/mocha": "^5.2.4",
    "@types/redux": "^3.6.0",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.5",
    "gsap": "^2.1.2",
    "mocha": "^6.0.2",
    "node-sass": "^4.9.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.4",
    "rxjs-compat": "^6.5.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "spa-http-server": "^0.9.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.0.0",
    "vue-mdc-adapter": "^0.18.2",
    "vue-rx": "^6.1.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
